<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Checked (dark): show moon icon -->
    <item android:state_checked="true"
        android:drawable="@drawable/ic_moon" />

    <!-- Unchecked (light): show sun icon -->
    <item android:drawable="@drawable/ic_sun" />
</selector>
